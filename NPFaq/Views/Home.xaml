                                            <DoubleAnimation Duration="0" To="1" Storyboard.TargetProperty="Opacity" Storyboard.TargetName="BackgroundRectangle"/>
                                        </Storyboard>
                                    </VisualState>
                                    <VisualState x:Name="UnfocusedSelected">
                                        <Storyboard>
                                            <DoubleAnimation Duration="0" To="1" Storyboard.TargetProperty="Opacity" Storyboard.TargetName="BackgroundRectangle"/>
                                            <ColorAnimation Duration="0" To="#FFE1E7EC" Storyboard.TargetProperty="(Fill).Color" Storyboard.TargetName="BackgroundRectangle"/>
                                        </Storyboard>
                                    </VisualState>
                                </VisualStateGroup>
                                <VisualStateGroup x:Name="ValidationStates">
                                    <VisualState x:Name="Valid"/>
                                    <VisualState x:Name="Invalid">
                                        <Storyboard>
                                            <ObjectAnimationUsingKeyFrames Duration="0" Storyboard.TargetProperty="Visibility" Storyboard.TargetName="BackgroundRectangle">
                                                <DiscreteObjectKeyFrame KeyTime="0" Value="Collapsed"/>
                                            </ObjectAnimationUsingKeyFrames>
                                            <DoubleAnimation Duration="0" To="1" Storyboard.TargetProperty="Opacity" Storyboard.TargetName="InvalidVisualElement"/>
                                        </Storyboard>
                                    </VisualState>
                                </VisualStateGroup>
                            </VisualStateManager.VisualStateGroups>
                            <Rectangle x:Name="BackgroundRectangle" Grid.ColumnSpan="2" Fill="#FFBADDE9" Opacity="0" Grid.RowSpan="2"/>
                            <Rectangle x:Name="InvalidVisualElement" Grid.ColumnSpan="2" Fill="#FFF7D8DB" Opacity="0" Grid.RowSpan="2"/>
                            <sdk:DataGridRowHeader x:Name="RowHeader" sdk:DataGridFrozenGrid.IsFrozen="True" Grid.RowSpan="3"/>
                            <sdk:DataGridCellsPresenter x:Name="CellsPresenter" Grid.Column="1" sdk:DataGridFrozenGrid.IsFrozen="True"/>
                            <sdk:DataGridDetailsPresenter x:Name="DetailsPresenter" Grid.Column="1" Grid.Row="1"/>
                            <Rectangle x:Name="BottomGridLine" Grid.Column="1" HorizontalAlignment="Stretch" Height="1" Grid.Row="2"/>
                        </sdk:DataGridFrozenGrid>
                    </ControlTemplate>
                </Setter.Value>
            </Setter>
        </Style>
    </sdk:Page.Resources>
    <sdk:Page.DataContext>
        <vm:HomeViewModel/>
    </sdk:Page.DataContext>
    <Grid x:Name="LayoutRoot">
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="150"/>
            <ColumnDefinition Width="*"/>
        </Grid.ColumnDefinitions>
        <Grid.RowDefinitions>
            <RowDefinition Height="35"/>
            <RowDefinition Height="*"/>
            <RowDefinition Height="35"/>
        </Grid.RowDefinitions>
        <Border Grid.ColumnSpan="3" Background="#FFFAFAFA"></Border>

        <StackPanel Orientation="Horizontal" Grid.Column="1" Visibility="{Binding Source={np:Static Member=local:App.CurrentUser},Path=IsLogin,Converter={StaticResource BooleanToVisibilityConverter}}">
            <np:ImageButton x:Name="AddBtn" ImageSource="/NPFaq;component/Images/add.png" Content="新增" ToolTipService.ToolTip="新增问题" Margin="5" Command="{Binding AskCommand}" Background="#FF74CE79" Foreground="White" MouseOverBrush="#FF71D977" PressedBrush="#FF71D977"/>
            <np:ImageButton x:Name="EditBtn" ImageSource="/NPFaq;component/Images/edit.png" Content="修改" ToolTipService.ToolTip="修改问题" Margin="5" Command="{Binding EditCommand}" Background="#FF11A0E4" Foreground="White" MouseOverBrush="#FF089adf" PressedBrush="#FF089adf"/>
            <np:ImageButton x:Name="DeleteBtn" ImageSource="/NPFaq;component/Images/delete.png" Content="删除" ToolTipService.ToolTip="批量删除问题" Margin="5" Command="{Binding DeleteCommand}" Foreground="White" Background="#FFD8514E" MouseOverBrush="#FFd24946" PressedBrush="#FFd24946"/>
        </StackPanel>
        <StackPanel Orientation="Horizontal" HorizontalAlignment="Right" Grid.Column="1">
            <TextBox x:Name="SearchTextBox" Width="150" Margin="5,5,0,5" KeyDown="SearchTextBox_KeyDown"/>
            <np:ImageButton x:Name="SearchBtn" ImageSource="/NPFaq;component/Images/search.png" Content="搜索" ToolTipService.ToolTip="搜索" Margin="0,5,5,5" Command="{Binding SearchCommand}" CommandParameter="{Binding Text, ElementName=SearchTextBox}"/>
        </StackPanel>
        <ListBox Grid.Row="1" x:Name="CategoryListBox" Grid.RowSpan="2" ItemsSource="{Binding Categorys}" SelectedItem="{Binding SelectedCategory, Mode=TwoWay}" Background="#FF555555" Foreground="White" ItemContainerStyle="{StaticResource ListBoxItemStyle1}">
            <ListBox.ItemTemplate>
                <DataTemplate>
                    <TextBlock Text="{Binding CategoryName}" FontSize="14" HorizontalAlignment="Center" VerticalAlignment="Center"/>
                </DataTemplate>
            </ListBox.ItemTemplate>
        </ListBox>
        <sdk:DataGrid x:Name="QuestionDG" GridLinesVisibility="All" SelectionMode="Single" Margin="5,0" AlternatingRowBackground="White" RowBackground="#FFF4F4F4" Grid.Row="1" Grid.Column="1" ItemsSource="{Binding Questions}" SelectedItem="{Binding SelectedQuestion,Mode=TwoWay}" FontSize="12" AutoGenerateColumns="False" CanUserSortColumns="False" Style="{StaticResource DataGridStyle1}" RowStyle="{StaticResource DataGridRowStyle1}" ColumnHeaderStyle="{StaticResource DataGridColumnHeaderStyle2}">
            <sdk:DataGrid.RowDetailsTemplate>
                <DataTemplate>
                    <StackPanel>
                        <ListBox ItemsSource="{Binding faq_answer}" BorderThickness="0" Padding="{Binding Source={np:Static Member=local:App.CurrentUser},Path=IsLogin,Converter={StaticResource BoolToMarginConverter}}">
                            <ListBox.ItemTemplate>
                                <DataTemplate>
                                    <StackPanel Orientation="Horizontal">
                                        <TextBlock Text="{Binding Converter={StaticResource IndexConverter}, RelativeSource={RelativeSource AncestorType=ListBoxItem, Mode=FindAncestor},StringFormat='{}{0}、'}" HorizontalAlignment="Center" Width="25"/>
                                        <TextBlock Text="{Binding Answer}" TextWrapping="Wrap" MaxWidth="1000"/>
                                    </StackPanel>
                                </DataTemplate>
                            </ListBox.ItemTemplate>
                        </ListBox>
                        <StackPanel Visibility="{Binding Attachs,Converter={StaticResource CollectionToVisibilityConverter}}">
                            <StackPanel Orientation="Horizontal">
                                <Image Source="/NPFaq;component/Images/attach.png" Width="15" Height="15" Margin="{Binding Source={np:Static Member=local:App.CurrentUser},Path=IsLogin,Converter={StaticResource BoolToMarginConverter}}"></Image>
                                <TextBlock Margin="3,0" Text="{Binding OtherAttachs.Count,StringFormat='附件 （{0}个）'}" VerticalAlignment="Center"/>
                            </StackPanel>
                            <ListBox ItemsSource="{Binding OtherAttachs}" BorderThickness="0"  Padding="{Binding Source={np:Static Member=local:App.CurrentUser},Path=IsLogin,Converter={StaticResource BoolToMarginConverter}}">
                                <ListBox.ItemsPanel>
                                    <ItemsPanelTemplate>
                                        <toolkit:WrapPanel/>
                                    </ItemsPanelTemplate>
                                </ListBox.ItemsPanel>
                                <ListBox.ItemTemplate>
                                    <DataTemplate>
                                        <Grid>
                                            <Grid.RowDefinitions>
                                                <RowDefinition Height="25"/>
                                                <RowDefinition Height="25"/>
                                            </Grid.RowDefinitions>
                                            <Grid.ColumnDefinitions>
                                                <ColumnDefinition Width="30"/>
                                                <ColumnDefinition Width="*"/>
                                            </Grid.ColumnDefinitions>
                                            <Image Grid.RowSpan="2" Source="{Binding Thumbnail,Converter={StaticResource BytesToImageSourceConverter}}"></Image>
                                            <TextBlock Margin="5,5,0,0" Grid.Column="1" Text="{np:MultiBinding Source1={Binding Path},Source2={Binding Size,Converter={StaticResource LongToStringConverter}},StringFormat='{}{0}  ({1})'}"/>
                                            <HyperlinkButton Margin="5,5,0,0" HorizontalAlignment="Left" Grid.Row="1" Grid.Column="1" Content="下载" NavigateUri="{np:MultiBinding Source1={np:Static Member=local:App.Root},Source2={Binding QuestionID}, Source3={Binding Path}, StringFormat='{}{0}ClientBin/Attach/{1}/{2}'}" TargetName="_blank"/>
                                        </Grid>
                                    </DataTemplate>
                                </ListBox.ItemTemplate>
                            </ListBox>
                        </StackPanel>
                    </StackPanel>
                </DataTemplate>
            </sdk:DataGrid.RowDetailsTemplate>
            <sdk:DataGrid.Columns>
                <sdk:DataGridCheckBoxColumn Header="全选" HeaderStyle="{StaticResource DataGridColumnHeaderStyle1}" Width="70" Binding="{Binding IsChecked}" np:DataGridColumn.Visibility="{Binding Source={np:Static Member=local:App.CurrentUser},Path=IsLogin,Converter={StaticResource BooleanToVisibilityConverter}}"/>
                <sdk:DataGridTextColumn Binding="{Binding Question}" IsReadOnly="True" Header="问题" Width="*"/>
                <sdk:DataGridTemplateColumn Header="操作" Width="150">
                    <sdk:DataGridTemplateColumn.CellTemplate>
                        <DataTemplate>
                            <StackPanel Orientation="Horizontal" Visibility="{Binding IsSelected,Converter={StaticResource BooleanToVisibilityConverter}}">
                                <np:ImageButton ToolTipService.ToolTip="图片预览" Margin="3" Content="预览" Height="25" ImageSource="/NPFaq;component/Images/preview.png" Command="{Binding DataContext.ShowAttachCommand, RelativeSource={RelativeSource AncestorType=sdk:DataGrid, Mode=FindAncestor}}" CommandParameter="{Binding}"/>
                                <np:ImageButton ToolTipService.ToolTip="删除" Margin="3" Content="删除" Height="25" ImageSource="/NPFaq;component/Images/delete1.png" Command="{Binding DataContext.DeleteCommand, RelativeSource={RelativeSource AncestorType=sdk:DataGrid, Mode=FindAncestor}}" CommandParameter="{Binding}" Visibility="{Binding Source={np:Static Member=local:App.CurrentUser},Path=IsLogin,Converter={StaticResource BooleanToVisibilityConverter}}"/>
                            </StackPanel>
                        </DataTemplate>
                    </sdk:DataGridTemplateColumn.CellTemplate>
                </sdk:DataGridTemplateColumn>
            </sdk:DataGrid.Columns>
        </sdk:DataGrid>
        <Grid Grid.Row="2" Grid.Column="1" Margin="5">
            <sdk:DataPager x:Name="DataPager" BorderThickness="0" Background="White" VerticalAlignment="Center" Source="{Binding Count}" PageSize="{Binding PageSize}">
                <i:Interaction.Triggers>
                    <np:EventToCommand EventName="PageIndexChanged" Command="{Binding ChangePageCommand}" CommandParameter="{Binding PageIndex, ElementName=DataPager}"/>
                </i:Interaction.Triggers>
            </sdk:DataPager>
            <TextBlock HorizontalAlignment="Left" VerticalAlignment="Center" Foreground="#FF505050" Text="{np:MultiBinding Source1={Binding ItemCount,ElementName=DataPager},Source2={Binding PageSize,ElementName=DataPager},StringFormat='共计{0}条记录,每页{1}条'}"/>
        </Grid>
    </Grid>
</sdk:Page>
